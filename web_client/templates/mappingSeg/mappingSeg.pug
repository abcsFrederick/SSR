        li.g-global-nav-li
          a.g-nav-link.qc-SAIP(g-name='qcSAIPProject' g-target='qcSAIPProject') 
            i.icon-download-cloud
            span SAIP
            i.icon-left-dir
            i.icon-right-dir
          //- ul.collapse.list-unstyled#projects
          //-   .g-hierarchy-breadcrumb-bar
          //-   .g-folder-list-container

        if SSR_Project.id
          li.g-global-nav-li
            a.g-nav-link.qc-Filesystem(g-id=SSR_Project.id g-name='qcSSRProject' g-target='qcSSRProject') 
              i.icon-file-archive
              span #{SSR_Project.get('name')}
              i.icon-left-dir
              i.icon-right-dir
          //- ul.collapse.list-unstyled#filesystem

        if user.id
          li.g-global-nav-li
            a.g-nav-link.qc-Girder(g-id=user.id g-name='qcUser' g-target='qcUser') 
              i.icon-edit
              span #{user.name()}
              i.icon-left-dir
              i.icon-right-dir

