.s-full-page-body
	width 100%

.footer
	position fixed
	right 0
	bottom 0
	color black
	text-align right
	opacity 0.5
	font-size 11px
	z-index 0
	
.category
	margin 0
	padding 0
	background-color #f1f1f1
	overflow auto
	border solid 1px
	border-radius 5px
	border-color lightgray
.dbContent
	min-height 0
	padding-right 0
	padding-left 0
.content

// globe girder template style
.g-top-level-list-title-container
	overflow inherit
	position absolute
.g-collection-list-entry .g-collection-right-column
	width 245px
#patients
	padding-right 5px
	padding-left 5px
#studies
	padding-right 5px
	padding-left 5px
#series
	padding-right 5px
	padding-left 5px
#analysis
	background-color #f1f1f1
	min-height 0
	border solid 1px
	border-radius 5px
	border-color #d3d3d3

#Loading
	display none
	position fixed
	z-index 100
	left 0
	top 0
	width 100%
	height 100%
	overflow auto
	background-color rgb(0, 0, 0)
	background-color rgba(0, 0, 0, 0.4)

.loadingIcon
	font-size 300px
	text-align center
	height 100%
	padding 100px 0
