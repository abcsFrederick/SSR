a#backward(style='display:inline') #[span.icon-left-open]
a.preview-allImages-dropdown-link(style='display:inline', data-toggle='dropdown')
  | #{currentImage} #[span.icon-down-open]
a#forward(style='display:inline') #[span.icon-right-open]
ul#currentImage.dropdown-menu(role='menu')
  if fromFilesystem
    each name in allImagesName
      li.dropdown-submenu.imageSelected(id=name.id)
        a #{name.get('name')}
  else if fromSaipArchive
    each name,index in allImagesName
      li.dropdown-submenu.imageSelected(id=name.get('id'))
        a #{name.get('series_description')}
  else
    each name in allImagesName
      li.dropdown-submenu.imageSelected(id=name.id)
        a #{name.get('name')}
