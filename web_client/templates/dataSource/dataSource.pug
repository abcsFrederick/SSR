			if SSR_Project.id
				li.g-global-nav-li
					a.g-nav-link.ds-Filesystem(g-id=SSR_Project.id g-name='dsSSRProject' g-target='dsSSRProject') 
						i.icon-file-archive
						span #{SSR_Project.get('name')}
						i.icon-left-dir
						i.icon-right-dir
				//- ul.collapse.list-unstyled#filesystem

			if user.id
				li.g-global-nav-li
					a.g-nav-link.ds-Girder(g-id=user.id g-name='dsUsers' g-target='dsUsers') 
						i.icon-edit
						span #{user.name()}
						i.icon-left-dir
						i.icon-right-dir
				//- ul.collapse.list-unstyled#girder
				//- 	.ds-Girder-Container

		ul.viewButton(style='list-style: none;')
			button.btn.btn-success#startView
				i.icon-eye
				a View
